{
  "name": "penh/settings",
  "keywords": ["settings", "persistent settings", "laravel"],
  "description": "Persistent settings package for Laravel framework.",
  "authors": [
    {
      "name": "Edvinas Kruƒças",
      "email": "edv.krucas@gmail.com"
    }
  ],

  "extra": {
    "laravel": {
      "providers": [
        "Krucas\\Settings\\Providers\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "Krucas\\Settings\\Facades\\Settings"
      }
    }
  },

  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0.0|^7.0.0|^8.0.0",
    "illuminate/console": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0.0|^7.0.0|^8.0.0",
    "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0.0|^7.0.0|^8.0.0",
    "illuminate/filesystem": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0.0|^7.0.0|^8.0.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "8"
  },
  "autoload": {
    "psr-0": {
      "Krucas\\Settings\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
